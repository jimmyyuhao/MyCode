<api name="constant">
  <doc>Constant Expressions</doc>

  <!-- constants -->
  <type name="constant" base="tree">
  </type>

  <!-- "cst" vs "constant" -->
  <type name="complex_constant" base="constant">
  </type>
  <type name="fixed_constant" base="constant">
  </type>
  <type name="integer_constant" base="constant">
  </type>
  <type name="ptrmem_constant" base="constant">
  </type>
  <type name="real_constant" base="constant">
  </type>
  <type name="string_constant" base="constant">
    <attribute name="char_ptr" kind="string"/>
  </type>
  <type name="vector_constant" base="constant">
  </type>
</api>